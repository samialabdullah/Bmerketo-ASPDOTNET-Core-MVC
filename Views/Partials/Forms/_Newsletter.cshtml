@model NewsletterViewModel

@if (Model != null)
{
    <section class="newsletter">
        <div class="container">
            <form>
                <input type="text" placeholder="@Model.Placeholder" maxlength="64">
                <button class="btn-theme btn-rounded"><i class="fa-sharp fa-solid fa-paper-plane fa-beat"></i></button>
            </form>
        </div>
    </section>
}